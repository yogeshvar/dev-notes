{
    "python.linting.pylintEnabled": true,
    "python.linting.enabled": true,
    "files.associations": {
        "*.inc": "php",
        "ostream": "cpp",
        "vector": "cpp",
        "iostream": "cpp",
        "__tuple": "cpp",
        "array": "cpp",
        "iosfwd": "cpp",
        "algorithm": "cpp"
    },
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++11 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    },
    "C_Cpp.errorSquiggles": "Disabled"
}